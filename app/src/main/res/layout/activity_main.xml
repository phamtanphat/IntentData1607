<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    tools:context=".MainActivity">

    <Button
        android:text="Intent String"
        android:id="@+id/buttonIntentString"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:layout_below="@id/buttonIntentString"
        android:text="Intent Number"
        android:id="@+id/buttonIntentNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:layout_below="@id/buttonIntentNumber"
        android:text="Intent Arraylist"
        android:id="@+id/buttonIntentArraylist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>